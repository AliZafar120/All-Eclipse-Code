import java.awt.Graphics;


public class House extends CompositeShape{

private int x1,y1,height,width;
private Graphics g;
	
public House(int x1,int y1,Graphics g) {
	// TODO Auto-generated constructor stub
	this.x1=x1;
	this.y1=y1;
	this.height=60;
	this.width=60;
	
	
	buildShape();
	
}

public House() {
	// TODO Auto-generated constructor stub
}

public House(int x1, int y1, int height, int width, Graphics g) {
	
	this.x1 = x1;
	this.y1 = y1;
	this.height = height;
	this.width = width;
	this.g = g;
	buildShape();
}

@Override
	public void buildShape() {
		// TODO Auto-generated method stub
	shapes.add(new Rectangle(x1, y1, this.height, this.width, g));
	shapes.add(new Triangle(x1, y1+height, x1+width, y1+height, g));
//	shapes.add(x)
	
		
	}


}
